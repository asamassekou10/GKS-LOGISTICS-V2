    <!-- Hero Section -->
    <section class="hero-section">
        <div class="hero-background">
            <img src="/assets/banner.png" alt="GKS Logistics Team Background" class="hero-bg-image">
            <div class="hero-overlay"></div>
        </div>
        <div class="container">
            <div class="hero-content">
                <div class="back-button">
                    <a href="news.html" class="btn-back">
                        <i class="fas fa-arrow-left"></i>
                        <span data-translate="back-to-news">Retour aux Actualités</span>
                    </a>
                </div>
                <div class="team-hero">
                    <h1 class="team-title" data-translate="team-title">Notre Équipe</h1>
                    <p class="team-subtitle" data-translate="team-subtitle">Découvrez les professionnels passionnés qui font de GKS Logistics un leader de la logistique en Afrique</p>
                    <div class="team-stats">
                        <div class="stat">
                            <span class="stat-number" data-translate="stats-20-plus">20+</span>
                            <span class="stat-label" data-translate="team-members">Membres d'équipe</span>
                        </div>
                        <div class="stat">
                            <span class="stat-number" data-translate="stats-10-plus">10+</span>
                            <span class="stat-label" data-translate="years-experience">Années d'expérience</span>
                        </div>
                        <div class="stat">
                            <span class="stat-number" data-translate="stats-10-plus">10+</span>
                            <span class="stat-label" data-translate="countries">Pays couverts</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
